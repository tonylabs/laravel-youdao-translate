{
	"name": "tonylabs/laravel-youdao-translate",
	"description": "Youdao translate API for Laravel",
	"homepage": "https://github.com/tonylabs/laravel-youdao-translate",
	"license": "MIT",
	"minimum-stability": "stable",
	"authors": [
		{
			"name": "TONYLABS TECH CO., LTD.",
			"email": "tony.wang@tonylabs.com"
		}
	],
	"require": {
		"php": "^8.0",
		"illuminate/http": "^9.0",
		"illuminate/support": "^9.0",
		"spatie/laravel-ray": "^1.29"
	},
	"autoload": {
		"psr-4": {
			"TONYLABS\\Translate\\": "src"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"aliases": {
				"Translate": "TONYLABS\\Translate\\Support\\Facades\\Translate"
			}
		}
	}
}